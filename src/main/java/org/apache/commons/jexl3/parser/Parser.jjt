    | < null_assign : "?=" >
    | < ne_assign : ":=" >
    | < increment : "++" >
    | < decrement : "--" >
    | < plus : "+" >
    | < minus : "-" >
    | < unary_minus : "\u2212" >
    | < mult : "*" | "\u22C5" >
    | < div : "/" >
    | < mod : "%" >
    | < unot : "\u00AC" >
    | < and : "&" >
    | < or : "|" >
